a = 1
while a <= 20:
    for i in range(2,a-1):
        if a%i == 0:
            a += 1
        else:
            print(a)

for a in range(2,20):
    for i in range(2,a-1):
        if a%i == 0:
            break
    else:
        print(a)

a = 1
while a <= 20:
    a += 1
    for i in range(2, a):
        if a%i == 0:
            break
    else:
        print(a)
